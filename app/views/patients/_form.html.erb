<div class="container">

<h3>Add images </h3>
<%= form_for(Image.new) do |f| %>
    <%= f.hidden_field :patient_id, value: @patient.id %>
  <div class="field">
    Image Description:<br>
    <%= f.text_field :image_desc %>
  </div>
  <div class="filefield">
  Attach your image file:
  <%= f.file_field :image_file, multiple: true %>
  </div>
<br>

  <div class="actions">
    <%= f.submit %>
  </div>

<% end %>
<%= link_to 'Back', patients_path %>
</div>